{
    "name": "node-express-server",
    "version": "0.0.1-SNAPSHOT",
    "description": "Simple node + express server",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/ibbatta/node-express-server.git"
    },
    "bugs": {
        "url": "https://github.com/ibbatta/node-express-server/issues"
    },
    "author": "Maurizio Battaghini <maurizio.battaghini@gmail.com>",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">= 9.10.x",
        "yarn": ">= 1.9.x",
        "npm": ">= 6.3.x"
    },
    "scripts": {
        "start": "NODE_ENV=production node --optimize_for_size --max_old_space_size=128 index.js --display-error-details",
        "start:dev": "NODE_ENV=develop nodemon index.js --display-error-details",
        "lint": "eslint --fix **/*.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint"
        }
    },
    "dependencies": {
        "bcrypt": "^3.0.1",
        "body-parser": "^1.18.3",
        "compression": "^1.7.3",
        "cors": "^2.8.4",
        "dotenv": "^6.0.0",
        "express": "^4.16.3",
        "express-session": "^1.15.6",
        "helmet": "^3.13.0",
        "jsonwebtoken": "^8.3.0",
        "mongoose": "^5.2.17"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.6",
        "babel-jest": "^23.6.0",
        "babel-loader": "^7.1.5",
        "babel-preset-env": "^1.7.0",
        "babel-register": "^6.26.0",
        "chalk": "^2.4.1",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "17.0.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-prettier": "3.0.0",
        "husky": "1.1.0",
        "nodemon": "^1.18.4",
        "prettier": "1.14.3"
    }
}