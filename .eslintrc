{
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "no-console": 1,
        "prettier/prettier": "error"
    }
}